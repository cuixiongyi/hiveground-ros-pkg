<launch>
   

	<!-- HgROS -->
	<node name="HgROS" pkg="hg_cpp" type="hg_ros" output="screen">	
		<rosparam file="$(find hg_cpp)/params/test.yaml" command="load" />
		<param name="robot_description/ve026a" textfile="$(find ve026a_description)/robot/ve026a.urdf" />
		<param name="simulate" value="false"/>
  </node>
 
  <node pkg="robot_state_publisher" type="state_publisher" name="rob_st_pub" >
      <remap from="robot_description" to="/HgROS/robot_description/ve026a" />
      <remap from="joint_states" to="/HgROS/joint_states" /> -->
  </node>
  
  <!--

  <node pkg="rviz" type="rviz" name="rviz">
      <remap from="robot_description" to="/HgROS/robot_description/ve026a" />
  </node>
    
  -->
 
</launch>
