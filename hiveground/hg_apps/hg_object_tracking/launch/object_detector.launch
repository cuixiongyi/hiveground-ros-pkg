<launch>
  <node pkg="hg_object_tracking" type="object_tracking" name="object_tracking" output="screen">
    <!--remap from="~cloud_in" to="passthrough_y/output" /-->
    <remap from="~cloud_in" to="/vp6242/self_filtered_cloud" />
    <rosparam>                  
      sac_distance_threshold: 0.025
      ec_cluster_tolerance: 0.05
      ec_min_size: 100
      ec_max_size: 2000
      area_x_min: 0.1
      area_x_max: 1.0
      area_y_min: -0.6
      area_y_max: 0.6
      area_z_min: 0.1
      area_z_max: 1.0
      arm_min_cluster_size: 50
      plam_max_cluster_size: 250
      arm_eigen_value_ratio: 4
    </rosparam>
  </node>
</launch>  
 