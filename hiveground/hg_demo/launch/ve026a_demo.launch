<launch>
  <arg name="is_simulated" default="true"/>  
  
  <param name="robot_description" textfile="$(find ve026a_description)/robot/ve026a.urdf"/>
  
  <node name="ve026a" pkg="hg_cpp" type="controller_node" output="screen">
    <param name="is_simulated" value="$(arg is_simulated)"/>
    <param name="loop_rate" value="100.0"/>
    <param name="joint_publish_rate" value="25.0"/>
    <param name="diagnostic_publish_rate" value="10.0"/>    
    <rosparam file="$(find hg_demo)/parameter/ve026a_demo.yaml" command="load" />             
  </node>
  
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher">  
  </node>     
</launch>
